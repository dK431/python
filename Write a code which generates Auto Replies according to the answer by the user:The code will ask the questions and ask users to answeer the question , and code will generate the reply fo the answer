prcnt = int(input("QUERRY 4 : What is you'r HSC Percentage : "))
if prcnt <=60 :
    print("REPLY : ! OH ! " , prcnt , " is you'r HSC Percentage")
elif prcnt >=60:
    print("REPLY : ! WOW ! " , prcnt , " is you'r HSC Percentage")

